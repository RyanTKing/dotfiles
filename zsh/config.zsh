# ZSH Colors
export LSCOLORS="exfxcxdxbxegedabagacad"
export CLICOLOR=true

# ZSH Options
setopt autolist
setopt cdablevars
setopt autonamedirs
setopt histignoredups
setopt listtypes
setopt nolistbeep
setopt complete_aliases
setopt extendedglob

# Enable Vim Keys
bindkey -v
bindkey '^R' history-incremental-search-backward
